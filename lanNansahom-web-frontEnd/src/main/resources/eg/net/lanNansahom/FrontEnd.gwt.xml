<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='FrontEnd'>
 
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User'/>
  <inherits name="com.google.gwt.resources.Resources" />
  
  <!-- Other module inherits                                      -->
  <inherits name="eg.net.gxt.GXTBase" />
  <inherits name='com.extjs.gxt.themes.Themes' />
  <inherits name="com.reveregroup.gwt.imagepreloader.ImagePreloader" />
  <inherits name="com.google.gwt.precompress.Precompress"/>
  
  <!-- Specify the app entry point class.                         -->
  <entry-point class='eg.net.lanNansahom.client.FrontEnd'/>

  <!-- Specify the paths for translatable code                    -->
  <source path='client'/>
  <source path='shared'/>
  <extend-property name="locale" values="ar_EG"/>
</module>